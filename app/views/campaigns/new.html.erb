<%= form_for @campaign do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :goal %>
    <%= f.number_field :goal %>

    <%= f.label :picture %>
    <%= f.text_field :picture %>

    <%= f.submit "Create Campaign" %>

<% end %>

    